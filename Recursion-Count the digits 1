import java.util.*;
public class Solution {
    static int count(int n) {
        int count = 0;
        if (n == 0) {
            return 1;
        }
       else
        while (n != 0) {
            n = n/10;
            count++;
        }
        return count;
    }
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        int res = count(n);
        System.out.println("The Count the digits: " + res);
    }
}
