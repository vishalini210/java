import java.util.*;
public class Solution{
    public static void main(String []args){
        Scanner sc=new Scanner(System.in);
        int a=sc.nextInt();
        if(a<1 || a>50)
        {
        System.out.print("Enter a Valid Input!!!!!!!!!!!!!!!!!!!!!!!!");
        }
        else if(a>=1 || a<=50){
        System.out.println("The first "+a+" Natural Numbers are:");
        for(int i=1;i<=a;i++){
           System.out.print(i+" ");
                
        }
    }
        
    }
}
