import java.util.*;
public class Solution{
public static void main(String []args){
    Scanner sc=new Scanner(System.in);
    int n=sc.nextInt();
    if(n<=0||n>=1000000000){
        System.out.print("Invalid Input");
    }else{
        int rev=0;

        while(n>0){
            int rem=n%10;

        rev=(rev*10)+rem;
            n=n/10;
        
        }
    System.out.println(rev);
}
}}
