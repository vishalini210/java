import java.util.*;
public class Solution{
    public static void main(String []args){
        Scanner sc=new Scanner(System.in);
        int size=sc.nextInt();
        int arr[]=new int[size];
        
        for(int i=0;i<size;i++){
           arr[i]=sc.nextInt();
        }
        
         int n=sc.nextInt();
        boolean found = false;
        for(int i=0;i<size;i++){
            if(arr[i]==n){
                found =true;
                break;
            }
        }
    if (found)
            System.out.println(n + " is presented in an array.");
    else
            System.out.println(n + " is not presented in an array.");        
    
    }
}
